#include<stdio.h>
#include<stdlib.h>
#include"queue.h"

Element* create_queue_element(int valeur) {
	Element* nouvel_element = (Element*) malloc(sizeof(Element));
	nouvel_element->contenu = valeur;
	nouvel_element->suivant = NULL;
	return nouvel_element;
}
