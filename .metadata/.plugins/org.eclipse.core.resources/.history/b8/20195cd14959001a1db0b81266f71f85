/*
 *****************************************************************************

 *****************************************************************************
 */

#include<stdio.h>
typedef struct {
	int R;
	int G;
	int B;
} RGB;



void SetLedColor( int LEDNum, RGB color) {
	if (color.R != 0 && color.G == 0 && color.B == 0) {
		printf("RED %d",color.R);
	} else if (color.R == 0 && color.G != 0 && color.B == 0) {
		printf("Green %d",color.G);
	} else if (color.R == 0 && color.G == 0 && color.B != 0) {
		printf("Blue %d",color.B);
	} else if (color.R == color.G && color.G == color.B && color.R != 0) {
		printf("White %d",color.R);
	} else if (color.R !=0 && color.G == 0 && color.B !=0 ) {
		printf("Purple %d %d",color.R, color.B);
	} else if (color.R == color.G && color.B ==0 ) {
		printf("yellow %d",color.R);
	}
}


int main(void){

	RGB tabline[7] = {{0,255,0},{255,255,255},{0,0,255},{255,0,0},{255,0,255},{255,255,0},{0,0,0}};

	for (int i = 0; i < 6; i++) {
		SetLedColor( i + 1, tabline[i]);
	}



	return 0;
}
