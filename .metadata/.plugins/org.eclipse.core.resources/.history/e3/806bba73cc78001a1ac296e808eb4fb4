#ifndef FIFO_H_
#define FIFO_H_


typedef struct Element Element;
struct Element {
	int nombre;
	Element *suivant;
};


typedef struct File File;
struct File {
	Element *premier;
};

void empiler(File *file, int nvNombre);

int depiler(File *file);

void afficherPile(File *file);

File* initialiser();

#endif /* FIFO_H_ */
