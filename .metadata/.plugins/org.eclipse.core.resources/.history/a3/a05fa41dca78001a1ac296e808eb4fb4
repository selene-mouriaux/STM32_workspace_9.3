#ifndef FIFO_H_
#define FIFO_H_


typedef struct Element Element;
struct Element
{
    int nombre;
    Element *suivant;
};

typedef struct Pile Pile;
struct Pile
{
    Element *premier;
};

void empiler(Pile *pile, int nvNombre);

int depiler(Pile *pile);

void afficherPile(Pile *pile);

Pile* initialiser();

#endif /* FIFO_H_ */
