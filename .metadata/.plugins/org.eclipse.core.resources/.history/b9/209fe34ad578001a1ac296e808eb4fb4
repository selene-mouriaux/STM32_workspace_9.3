#ifndef FIFO_H_
#define FIFO_H_


typedef struct Element Element;
struct Element {
	int nombre;
	Element *suivant;
};


typedef struct Pile Pile;
struct Pile {
	Element *premier;
};

void empiler(Pile *file, int nvNombre);

int depiler(Pile *file);

void afficherPile(Pile *file);

void initialiser(Pile* pile);

#endif /* FIFO_H_ */
